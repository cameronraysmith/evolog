@Article{	  hindley2006,
  abstract	= {The formal systems that are nowadays called
		  $\lambda$-calculus and combinatory logic were both invented
		  in the 1920s, and their aim was to describe the most basic
		  properties of function–abstraction, application, and
		  substitution in a very general setting. In
		  $\lambda$-calculus, the concept of abstraction has been
		  taken as primitive; but in combinatory logic, it has been
		  defined in terms of certain primitive operators called
		  “basic combinators.” This chapter discusses the history
		  of these two topics. Today, $\lambda$ and combinatory logic
		  (CL) are used extensively in higher-order logic and
		  computing. Rather like the chassis of a bus, which supports
		  the vehicle but is unseen by its users, versions of
		  $\lambda$ or CL underpin several important logical systems
		  and programming languages. The chapter discusses earlier
		  work in chronological order and classifies later
		  developments. The later developments are discussed in the
		  chapter to show something of the contexts in which
		  $\lambda$ and CL are being used.},
  author	= {Cardone, Felice and Hindley, J Roger},
  doi		= {10.1016/S1874-5857(09)70018-4},
  file		= {:home/cameron/Mendeley/Cardone, Hindley/2009/Cardone,
		  Hindley - 2009 - Lambda-Calculus and Combinators in the
		  20th Century.pdf:pdf},
  isbn		= {9780444516206},
  issn		= {18745857},
  journal	= {Handbook of the History of Logic},
  keywords	= {combinatory logic,game semantics,historical,lambda
		  calculus},
  mendeley-tags	= {combinatory logic,game semantics,historical,lambda
		  calculus},
  pages		= {723--817},
  title		= {{Lambda-Calculus and Combinators in the 20th Century}},
  volume	= {5},
  year		= {2009}
}

@Article{	  mossio2009,
  abstract	= {In this paper, we propose a mathematical expression of
		  closure to efficient causation in terms of lambda-calculus;
		  we argue that this opens up the perspective of developing
		  principled computer simulations of systems closed to
		  efficient causation in an appropriate programming language.
		  An important implication of our formulation is that, by
		  exhibiting an expression in lambda-calculus, which is a
		  paradigmatic formalism for computability and programming,
		  we show that there are no conceptual or principled problems
		  in realizing a computer simulation or model of closure to
		  efficient causation. We conclude with a brief discussion of
		  the question whether closure to efficient causation
		  captures all relevant properties of living systems. We
		  suggest that it might not be the case, and that more
		  complex definitions could indeed create crucial some
		  obstacles to computability.},
  author	= {Mossio, Matteo and Longo, Giuseppe and Stewart, John},
  doi		= {10.1016/j.jtbi.2008.12.012},
